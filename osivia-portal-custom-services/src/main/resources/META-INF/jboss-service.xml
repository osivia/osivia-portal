          xmbean-code="org.jboss.portal.jems.as.system.JBossServiceModelMBean">
			 <depends optional-attribute-name="Tracker" proxy-type="attribute">osivia:service=Tracker</depends>
      <xmbean/>
   </mbean>
   
     <mbean code="org.osivia.portal.core.pagemarker.PortalCommandFactory"
          name="portal:commandFactory=DefaultPortal" xmbean-dd=""
          xmbean-code="org.jboss.portal.jems.as.system.JBossServiceModelMBean">
      <xmbean/>
      <depends optional-attribute-name="NextFactory" proxy-type="attribute">portal:commandFactory=Delegating</depends>
      <depends optional-attribute-name="Container" proxy-type="attribute">portal:container=PortalObject</depends>
   </mbean>
	
	
	   <mbean code="org.osivia.portal.core.imports.ImportInterceptor"
          name="portal:service=Interceptor,type=Server,name=Import" xmbean-dd=""
          xmbean-code="org.jboss.portal.jems.as.system.JBossServiceModelMBean">
      <xmbean/>
   </mbean>
   
   	  <mbean code="org.osivia.portal.core.migration.MigrationService"
          name="osivia:service=MigrationService" xmbean-dd=""
          xmbean-code="org.jboss.portal.jems.as.system.JBossServiceModelMBean">
      <xmbean/>
        <depends optional-attribute-name="PortalObjectContainer" proxy-type="attribute">portal:container=PortalObject
      </depends>
   </mbean>
	
	
	<mbean code="org.osivia.portal.core.customization.CustomizationService"
          name="osivia:service=CustomizationService" xmbean-dd=""
          xmbean-code="org.jboss.portal.jems.as.system.JBossServiceModelMBean">
      <xmbean/>
   </mbean>
   
	<mbean code="org.osivia.portal.core.cms.CMSServiceLocator"
          name="osivia:service=CmsServiceLocator" xmbean-dd=""
          xmbean-code="org.jboss.portal.jems.as.system.JBossServiceModelMBean">
      <xmbean/>
   </mbean>

    <!-- Selection service -->
    <mbean
        code="org.osivia.portal.core.selection.SelectionService"
        name="osivia:service=SelectionService"
        xmbean-dd=""
        xmbean-code="org.jboss.portal.jems.as.system.JBossServiceModelMBean">
        <xmbean />
    </mbean>

    <!-- Internationalization service -->
    <mbean
        code="org.osivia.portal.core.internationalization.InternationalizationService"
        name="osivia:service=InternationalizationService"
        xmbean-dd=""
        xmbean-code="org.jboss.portal.jems.as.system.JBossServiceModelMBean">
        <xmbean />
        <depends
            optional-attribute-name="CustomizationService"
            proxy-type="attribute">osivia:service=CustomizationService</depends>
    </mbean>
    
    <!-- Notifications service -->
    <mbean
        code="org.osivia.portal.core.notifications.NotificationsService"
        name="osivia:service=NotificationsService"
